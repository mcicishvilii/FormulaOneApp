<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.example.formulaone.ui.welcomeFragment.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcomeFragment_to_createAccountFragment"
            app:destination="@id/createAccountFragment" />
        <action
            android:id="@+id/action_welcomeFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
        <action
            android:id="@+id/action_welcomeFragment_to_signInFragment"
            app:destination="@id/signInFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountFragment"
        android:name="com.example.formulaone.ui.createAccountFragments.firstName.CreateAccountFragment"
        android:label="fragment_create_account"
        tools:layout="@layout/fragment_create_account" >
        <action
            android:id="@+id/action_createAccountFragment_to_createAccountFragmentStepTwo"
            app:destination="@id/createAccountFragmentStepTwo" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.formulaone.ui.mainFragment.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_driversFragment"
            app:destination="@id/driversFragment" />
        <action
            android:id="@+id/action_mainFragment_to_teamsFragment"
            app:destination="@id/teamsFragment" />
        <action
            android:id="@+id/action_mainFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="com.example.formulaone.ui.signInFragment.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in" >
        <action
            android:id="@+id/action_signInFragment_to_signInFragmentStepTwo"
            app:destination="@id/signInFragmentStepTwo" />
    </fragment>
    <fragment
        android:id="@+id/signInFragmentStepTwo"
        android:name="com.example.formulaone.ui.signInFragment.SignInFragmentStepTwo"
        android:label="fragment_sign_in_fragment_step_two"
        tools:layout="@layout/fragment_sign_in_fragment_step_two" >
        <action
            android:id="@+id/action_signInFragmentStepTwo_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/createAccountFragmentStepTwo"
        android:name="com.example.formulaone.ui.createAccountFragments.lastName.CreateAccountFragmentStepTwo"
        android:label="fragment_create_account_fragment_step_two"
        tools:layout="@layout/fragment_create_account_fragment_step_two" >
        <action
            android:id="@+id/action_createAccountFragmentStepTwo_to_createAccountFragmentStepThree"
            app:destination="@id/createAccountFragmentStepThree" />
    </fragment>
    <fragment
        android:id="@+id/createAccountFragmentStepThree"
        android:name="com.example.formulaone.ui.createAccountFragments.dob.CreateAccountFragmentStepThree"
        android:label="fragment_create_account_fragment_step_three"
        tools:layout="@layout/fragment_create_account_fragment_step_three" >
        <action
            android:id="@+id/action_createAccountFragmentStepThree_to_createAccountFragmentStepFour"
            app:destination="@id/createAccountFragmentStepFour" />
    </fragment>
    <fragment
        android:id="@+id/createAccountFragmentStepFour"
        android:name="com.example.formulaone.ui.createAccountFragments.email.CreateAccountFragmentStepFour"
        android:label="fragment_create_account_fragment_step_four"
        tools:layout="@layout/fragment_create_account_fragment_step_four" >
        <action
            android:id="@+id/action_createAccountFragmentStepFour_to_createAccountFragmentStepFive"
            app:destination="@id/createAccountFragmentStepFive" />
    </fragment>
    <fragment
        android:id="@+id/createAccountFragmentStepFive"
        android:name="com.example.formulaone.ui.createAccountFragments.country.CreateAccountFragmentStepFive"
        android:label="fragment_create_account_fragment_step_five"
        tools:layout="@layout/fragment_create_account_fragment_step_five" >
        <action
            android:id="@+id/action_createAccountFragmentStepFive_to_createAccountFragmentStepSix"
            app:destination="@id/createAccountFragmentStepSix" />
    </fragment>
    <fragment
        android:id="@+id/createAccountFragmentStepSix"
        android:name="com.example.formulaone.ui.createAccountFragments.password.CreateAccountFragmentStepSix"
        android:label="fragment_create_account_fragment_step_six"
        tools:layout="@layout/fragment_create_account_fragment_step_six" >
        <action
            android:id="@+id/action_createAccountFragmentStepSix_to_mainFragment"
            app:destination="@id/mainFragment" />
        <argument
            android:name="countryName"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/driversFragment"
        android:name="com.example.formulaone.ui.navMenuFragments.drivers.DriversFragment"
        android:label="fragment_drivers"
        tools:layout="@layout/fragment_drivers">
        <argument
            android:name="ShowAppBar"
            android:defaultValue="true" />
    </fragment>
    <fragment
        android:id="@+id/teamsFragment"
        android:name="com.example.formulaone.ui.navMenuFragments.teams.TeamsFragment"
        android:label="fragment_teams"
        tools:layout="@layout/fragment_teams" >
        <argument
            android:name="ShowAppBar"
            android:defaultValue="true" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.formulaone.ui.navMenuFragments.settings.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <argument
            android:name="ShowAppBar"
            android:defaultValue="true" />
    </fragment>
</navigation>